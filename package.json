{
    "name": "node-demo",
    "version": "0.0.1",
    "description": "node 项目最佳实践",
    "icafe": "node-best-practices",
    "private": true,
    "dependencies": {
        "egg": "^2.2.1",
        "egg-scripts": "^2.5.0"
    },
    "devDependencies": {
        "@baidu/conventional-changelog-befe": "^1.2.1",
        "autod": "^3.0.1",
        "autod-egg": "^1.0.0",
        "commitizen": "^2.10.1",
        "conventional-changelog-cli": "^2.0.5",
        "cz-customizable": "^5.2.0",
        "egg-bin": "^4.3.5",
        "egg-ci": "^1.8.0",
        "egg-mock": "^3.14.0",
        "eslint": "^4.11.0",
        "eslint-config-egg": "^6.0.0",
        "webstorm-disable-index": "^1.2.0"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-node-demo",
        "stop": "egg-scripts stop --title=egg-server-node-demo",
        "dev": "egg-bin dev",
        "debug": "egg-bin debug",
        "test": "npm run lint -- --fix && npm run test-local",
        "test-local": "egg-bin test",
        "cov": "egg-bin cov",
        "lint": "eslint .",
        "ci": "npm run lint && npm run cov",
        "autod": "autod",
        "commit": "git cz",
        "changelog": "conventional-changelog -p @baidu/befe -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "version ": "npm run changelog"
    },
    "ci": {
        "version": "8"
    },
    "repository": {
        "type": "git",
        "url": "ssh://chenmanqing@icode.baidu.com:8235/baidu/hi-fe/node-demo"
    },
    "author": "chenmanqing",
    "license": "MIT",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-customizable"
        }
    }
}
